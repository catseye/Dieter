/* check that diff instances of equal(另, 另) can unify to diff types */

forward and(bool, bool): bool
forward equal(另, 另): bool
forward print(string): void

module example1

procedure thing(): void
  var i: int
  var j: int
  var s: string
  var t: string
begin
  if and(equal(i, j), equal(s, t)) then print("yes") else print("no")
end

end.
